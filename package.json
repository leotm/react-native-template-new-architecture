{
  "name": "myapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "allow-scripts",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "android": "react-native run-android --active-arch-only",
    "android:release": "react-native run-android --active-arch-only --variant release",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release",
    "ios:light": "xcrun simctl ui booted appearance light",
    "ios:dark": "xcrun simctl ui booted appearance dark",
    "doctor": "react-native doctor",
    "lint": "eslint . --ext ts,tsx,yml --cache",
    "lint:fix": "eslint . --ext ts,tsx,yml --cache --fix",
    "lint:config": "eslint --print-config index.ts",
    "prettier:check": "yarn prettier --check src/**/*.{ts,tsx,json} && yarn prettier --check *.{ts,tsx,json}",
    "prettier:write": "yarn prettier --write src/**/*.{ts,tsx,json} && yarn prettier --write *.{ts,tsx,json}",
    "check:all": "yarn tsc && yarn lint && yarn prettier:check && jest",
    "fix": "yarn lint:fix && yarn prettier:write",
    "test": "jest",
    "snap": "jest -u",
    "get-stories": "sb-rn-get-stories --config-path=./.storybook",
    "storybook-watcher": "sb-rn-watcher --config-path=./.storybook",
    "storybook-server": "react-native-storybook-server",
    "flipper-server": "flipper-server",
    "node-server": "ts-node node.ts",
    "deno-server": "deno run --allow-net deno.ts",
    "rename": "yarn react-native-rename",
    "clean": "react-native clean",
    "clean:proj": "react-native clean-project",
    "clean:proj-auto": "react-native clean-project-auto",
    "help": "react-native"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/checkbox": "0.5.20",
    "@react-native-community/datetimepicker": "7.0.1",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/native": "6.1.7",
    "@react-navigation/native-stack": "6.9.13",
    "@tanstack/react-query": "5.90.10",
    "react": "18.2.0",
    "react-native": "0.71.12",
    "react-native-codegen": "0.71.5",
    "react-native-gesture-handler": "2.9.0",
    "react-native-reanimated": "3.0.2",
    "react-native-safe-area-context": "4.5.5",
    "react-native-screens": "3.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-react": "7.18.6",
    "@babel/runtime": "7.21.5",
    "@curveball/core": "0.21.1",
    "@curveball/router": "0.6.0",
    "@lavamoat/allow-scripts": "3.4.1",
    "@lavamoat/git-safe-dependencies": "0.3.1",
    "@lavamoat/preinstall-always-fail": "2.1.1",
    "@react-native-community/cli": "11.3.7",
    "@react-native-community/eslint-config": "3.2.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-backgrounds": "6.5.16",
    "@storybook/addon-controls": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-ondevice-actions": "6.5.3",
    "@storybook/addon-ondevice-backgrounds": "6.5.3",
    "@storybook/addon-ondevice-controls": "6.5.3",
    "@storybook/addon-ondevice-knobs": "6.5.3",
    "@storybook/addon-ondevice-notes": "6.5.3",
    "@storybook/addons": "6.5.16",
    "@storybook/docs-tools": "6.5.16",
    "@storybook/eslint-config-storybook": "3.1.2",
    "@storybook/linter-config": "3.1.2",
    "@storybook/react": "6.5.16",
    "@storybook/react-native": "6.5.3",
    "@storybook/react-native-server": "6.5.3",
    "@swc/core": "1.3.76",
    "@swc/wasm": "1.3.76",
    "@tsconfig/react-native": "3.0.8",
    "@types/jest": "29.5.3",
    "@types/node": "18.19.130",
    "@types/react": "18.2.20",
    "@types/react-native": "0.72.2",
    "@types/react-test-renderer": "18.0.0",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.3",
    "babel-plugin-react-docgen-typescript": "1.5.1",
    "babel-plugin-transform-inline-environment-variables": "0.4.4",
    "eslint": "8.57.1",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "eslint-config-react-native-typescript": "2.2.7",
    "eslint-plugin-communist-spelling": "1.0.0",
    "eslint-plugin-deprecation": "1.5.0",
    "eslint-plugin-detox": "1.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "5.0.0",
    "eslint-plugin-simple-import-sort": "9.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.3.0",
    "eslint-plugin-yml": "1.19.0",
    "html-webpack-plugin": "5.5.3",
    "husky": "9.1.7",
    "jest": "29.5.0",
    "lint-staged": "16.2.7",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.8",
    "react-codemod": "5.4.4",
    "react-dom": "18.2.0",
    "react-native-clean-project": "4.0.3",
    "react-native-gradle-plugin": "0.71.19",
    "react-native-rename": "3.2.17",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "5.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.9.3",
    "webpack-cli": "5.0.2",
    "webpack-dev-server": "4.13.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "lib"
    ],
    "setupFiles": [
      "./jest/setup.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.12.0",
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "@storybook/react>@pmmmwh/react-refresh-webpack-plugin>core-js-pure": false,
      "@storybook/react>core-js": false,
      "@swc/core": false,
      "webpack-cli>webpack>watchpack>watchpack-chokidar2>chokidar>fsevents": false
    }
  }
}
